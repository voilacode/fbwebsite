{
  "name": "multiapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prerender": "npm run build && node scripts/prerender.js",
    "build:voilaco": "BUILD_TARGET=voilaco tsc && BUILD_TARGET=voilaco vite build",
    "build:voilaco:core": "CLIENT_SLUG=core npm run build:voilaco",
    "preview": "vite preview",
    "bundle": "npx uikit bundle",
    "serve": "npx uikit serve",
    "deploy": "npx uikit deploy",
    "predeploy": "npm run build:prerender",
    "ghpages": "gh-pages -d dist",
    "optimize-images": "node scripts/optimize-images.js"
  },
  "dependencies": {
    "@fontsource/caveat": "^5.2.8",
